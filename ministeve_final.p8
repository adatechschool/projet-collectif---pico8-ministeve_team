--player
function create_player()
	p={x=6,
	y=5,
	sprite=1,
	trash=0,
	final=0,
	anim=0,
	turn_l=true,
--ajout justine
	sprite_o=0,
	coule=true,gene=true,
	anim_t=1,
	ox=0, oy=0,
	start_ox=0,start_oy=0,
	dir_tir=1 --‚¨ÜÔ∏è1,‚û°Ô∏è2,‚¨áÔ∏è3,‚¨ÖÔ∏è4
	}
end

function player_move()
	newx = p.x
	newy = p.y
	if(btnp(‚¨ÖÔ∏è)) then 
		newx-=1
		p.turn_l=true
		p.dir_tir=4
	end
	if(btnp(‚û°Ô∏è)) then
	 newx+=1
	 p.turn_l=false
	 p.dir_tir=2
	end
	if btnp(‚¨áÔ∏è) then
	 newy+=1
	 p.dir_tir=3
	end
	if btnp(‚¨ÜÔ∏è) then
	 newy-=1
	 p.dir_tir=1
	end
	
	interact(newx,newy)
	
--on change le sprite des qu'on
--essaie de bouger p	
	if (p.x!=newx or p.y!=newy) then
			if(p.anim==0) then
				p.sprite=1
				p.anim=1
				else
					p.sprite=17
					p.anim=0
			end
		end
	
	if not check_flag(0,newx,newy) then
	--	p.x = newx
	--	p.y = newy		
	
		p.x = mid(0,newx,46)
		p.y = mid(0,newy,22)
		

	
	end 
end

